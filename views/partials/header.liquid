                </svg>
            </a>

        {% else %} {% comment %} general header {% endcomment %}
    {% endcase %}
    {% case v %}
        {% when "h" %} {% comment %} Home header {% endcomment %}
            <a href="#home" class="logo">
                <img src="/assets/mediahuis-high.webp" alt="Logo van {{ title }}">
            </a>
        {% else %} {% comment %} general header {% endcomment %}
        <a href="/station/{{thisstation}}" class="logo">
            <img src="/assets/radio-{{thisstation}}.png" alt="Logo van {{ thisstation }}">
        </a>
    {% endcase %}
        
    {% case v %}
        {% when "h" %} {% comment %} Home header {% endcomment %}
            </div> {% comment %} Close div with white shape on home header {% endcomment %}
        {% else %}
    
    {% endcase %} 
        
    
    {% case v %}
        {% when "o" %} {% comment %} Other header {% endcomment %}
            <nav class="desktop-show">
    

                <ul class="primary-nav">
                    {% comment %} home + radio + ontdek {% endcomment %}
                    <li><a href="/">Home</a></li>
                    <li><a href="#radio" class="active">Radio</a></li>
                    <li><a href="#ontdek">Ontdek</a></li>
                </ul>
        
            </nav>
