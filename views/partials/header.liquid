<header class="header-{{v}} radio-{{thisstation}}" id="aside-toggler">

    {% case v %}
        {% when "h" %} {% comment %} Home header {% endcomment %}
            <div class="shape-header-home"> {% comment %} Div with white shape on home header {% endcomment %}
        {% else %}
    
    {% endcase %} 
    {% case v %}
        {% when "o" %} {% comment %} Other header {% endcomment %}
